<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/test_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/llc_factory_reset_dialog"
        style="@style/FastStartFragment"
        android:layout_marginTop="50dp"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.MyApp.Button.Rounded"
        android:layout_width="279dp"
        android:layout_height="430dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_explanation_text_factory_reset_dialog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_p"
            android:layout_marginTop="@dimen/spacing_xxxxl"
            android:layout_marginEnd="@dimen/spacing_p"
            android:minHeight="48dp"
            android:text="@string/app_name"
            android:textIsSelectable="true" />

        <Button
            android:id="@+id/bFastStartService"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"></LinearLayout>

    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.appcompat.widget.LinearLayoutCompat>