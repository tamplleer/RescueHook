<?xml version="1.0" encoding="utf-8"?>


<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="279dp"
    android:layout_height="430dp"
    android:layout_gravity="center"
    android:background="@color/grey_1"
    android:backgroundTint="@color/grey_1"
    app:cardCornerRadius="@dimen/spacing_xxp"
    app:cardUseCompatPadding="true">



    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/cardContainer"
        android:name="com.whynotpot.rescuehook.themes.ThemeSimpleAlpha"
        android:layout_width="wrap_content"
        android:layout_height="430dp"
        android:layout_marginTop="20dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/spacing_xxs"
        android:paddingLeft="@dimen/spacing_p"
        android:text="@string/app_name"
        android:textColor="@color/blue_1" />
</androidx.cardview.widget.CardView>





